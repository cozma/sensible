{
	"manifest_version": 2,
	"name": "Sensible Block ",
	"version": "1.0",
	"description": "Chrome extension that replaces occurrences of bad language with a warning.",
	"content_scripts": 
	[
		{
			"matches": ["*://*/*"],
			"js": ["jquery-3.1.1.min.js", "underscore-min.js", "require.js", "firebase.js", "content_script.js"],
			"run_at": "document_end"
		}
	]
}

